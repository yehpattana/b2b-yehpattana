{
  
  "compilerOptions": {
    // "typeRoots": ["node_modules/@types/tailwindcss"],
    "target": "es2015",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/**/*.tsx",
    "**/**/*.ts",
    "*.tsx",
    "*.ts",
    "**/**/**/*.tsx",
    "**/**/**/*.ts",
    "**/**/**/**/*.tsx",
    "**/**/**/**/*.ts",
    "**/**/**/**/**/*.tsx",
    "**/**/**/**/**/*.ts",
    "**/**/**/**/**/**/*.tsx",
    "**/**/**/**/**/**/*.ts",
    "**/**/**/**/**/**/**/*.tsx",
    "**/**/**/**/**/**/**/*.ts",
    "**/**/**/**/**/**/**/**/*.tsx",
    "**/**/**/**/**/**/**/**/*.ts",
     "src/app/[customerName]/[menu]/products/list",
  ],
  "exclude": [
    "node_modules"
  ]
}
